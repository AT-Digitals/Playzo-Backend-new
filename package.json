{
  "name": "fabo-api",
  "version": "1.0.0",
  "description": "API for FABO website and admin panel",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "start": "npm run build && npm run watch",
    "build": "npm run clean && npm run build-ts && npm run eslint",
    "serve": "nodemon ./dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\"  \"npm run watch-ts\" \"npm run serve\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "test": "jest --passWithNoTests",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git@git.aftertutor.com:at-digitals/fabo-api.git"
  },
  "author": "Tamilarasu",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "0.10.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "0.8.1",
    "typedi": "^0.8.0",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/csurf": "^1.11.0",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.34",
    "@types/mongoose": "^5.10.1",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.14.9",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.13.0",
    "import-sort-parser-typescript": "^6.0.0",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "ts-watch": "^1.0.8",
    "typescript": "^4.1.2"
  }
}
